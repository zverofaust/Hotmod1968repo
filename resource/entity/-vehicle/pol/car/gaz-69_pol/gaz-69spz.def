{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(0.9))
	(include "/properties/selection/vehicle.inc" scale(0.8))
	{props "moto" "vision_lev01" "tractor" "tractor_light"}
	{able "emit_on_attack"}
	{able "emit_on_attack"}
	{PatherId "kubel"}
	{targetSelector "car_bullet"}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("fuel" args "fuel")
	("transmission" args "transmission")
	{volume "engine" {thickness 10}}
	
	(include "crew.ext")
	
	{sensor
		{visor "commander"
			{vision "human"}
			{bone "visor"}
		}
		{visor "driver"
			{vision "human"}
			{bone "visor"}
		}
		{visor "passenger"
			{vision "human"}
			{bone "visor"}
		}
	}


{extender "inventory"
		{box
			{item "repair_kit" 5}
			{item "pmd" "mine" 20}
			{item "tmd_b" "mine" 10}
			{item "mine_detector" 1}
			{item "pickaxe" 2}
			{item "satchel_charge_rus" 5}
			{item "wirecutters" 1}
		}
	}


	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(80) ;----------------------------------Max speed.
			speedatmaxturn(12) ;-------------------------Max speed at max turn.
			reverse(18) ;--------------------------------Max speed in Reverse.
			weight(1.32) ;-------------------------------Weight in Tonnes.
			power(54) ;----------------------------------Horse Power.
			fuel(60) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(170) ;---------------------------------Range in Kilometers.
		)
	}
	
	("glass" args "window1" 1)
	
	{Volume "Body"
		{thickness 4}
	}
	{Volume "engine"
		{thickness 4}
	}
	
	{extender "shield"
			{off}
	}

	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(80) ;----------------------------------Max speed.
			speedatmaxturn(12) ;-------------------------Max speed at max turn.
			reverse(18) ;--------------------------------Max speed in Reverse.
			weight(1.32) ;-------------------------------Weight in Tonnes.
			power(54) ;----------------------------------Horse Power.
			fuel(60) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(170) ;---------------------------------Range in Kilometers.
		)
	}
	
	{Extension "gaz-69.mdl"}

	{volume "shield"
		{able {nowounddecal}}
		{thickness 0.3}
	}
	{component "shield"	
		{tags "shield"}
		{RepairTime 20}
	}
	{extender "shield"
		{on}	
	}
}
