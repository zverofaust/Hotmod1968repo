{game_entity		;Ural-375D
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "multi_board" "truck" "vision_lev02" "tractor" "tractor_medium" "shell_eject" "flak"  "muzzle_20mm_25mm_auto"  "aa_medium"}
	{able "emit_on_attack"}
	{collider "truck"}
	{patherId "gazaaa"}


	;volume, component/tag, thickness
	{volume "body3"}
	
	;volume/component, thickness
	("glass" args "glass_doorl1" 1)
	("glass" args "glass_doorr1" 1)
	("glass" args "glass_front" 1)
	("fuel" args "fuel")
	("transmission" args "transmission")
	("gun" repair(30) thickness(15))
	
	;Crew 4: driver, commander, gunner, charger
	(include "/properties/tank_crew.ext")



	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)
	
	{placer 
	{place "driver" 				{group "crew"}}
	{place "commander" 				{group "crew"}}
	{place "gunner"		   {visor "main"}	{group "crew"}}
	{place "charger"	   {visor "around"}	{group "crew"}}
	}
	
        {sensor
    	{visor "gun"
        {vision "cannon_main"}
        {bone "foresight1"}
        }
        }

	{boarder
		

		{anm "driver"
			{forward	{begin "board_opel_driver"} {end "seat_driver_car"}}
			{reverse	{end "seat_driver_car"}}
		}
		{anm "commander"
			{forward	{begin "board_big-highcar"} {end "seat_passenger"}}
			{reverse	{end	"seat_passenger"}}
		}
		{anm "gunner"
			{forward	{begin "board_Is-2"} {end "AAgun_flak37_gunner"}}
			{reverse	{end	"emit_tank_2_hold"}}
		}
		{anm "charger"
			{forward	{begin "board_Is-2"} {end "AAgun_flak37_charger"}}
			{reverse	{end	"emit_tank_2_hold"}}
		}
		
	}

	("crew_human_visible_anm"	place(driver) 	  	door(emit1) door2(emit1) visor(driver) 	anm_left(driver) 		anm_right(driver))
	("crew_human_visible_anm"	place(commander)  	door(emit2) door2(emit2) visor(around) 	anm_left(commander)  		anm_right(commander))
	("crew_human_visible_anm"	place(gunner) 		door(emit4) door2(emit4) visor(main)   	anm_left(gunner)   		anm_right(gunner))			
	("crew_human_visible_anm"	place(charger) 	 	door(emit3) door2(emit3) visor(around)	anm_left(charger)  		anm_right(charger))	
	
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{extender "inventory"
		{box
			{item "bullet23_zu23 aphe" 2000}		;
			{item "bullet23_zu23 he" 2000}
			{item "satchel_charge_rus" 1}
		}
	}
	
		{Weaponry
		("restore_ik_time")
		{place "gun"
			{type "aa"}
			{Barrels 2}
			{weapon "23mm_zu-23-2" filling "bullet23_zu23 he" 250}
			{gunner "gunner"}
			{charger "charger"}			
			("hm68_abm_gun")	 
			{basic}
		}
	}
	
	{mass 8425}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(75) ;----------------------------------Max speed.
			speedatmaxturn(17) ;-------------------------Max speed at max turn.
			reverse(22) ;--------------------------------Max speed in Reverse.
			weight(8.4) ;------------------------------Weight in Tonnes.
			power(180) ;----------------------------------Horse Power.
			fuel(360) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(750) ;---------------------------------Range in Kilometers.
		)
	}

	{Extension "ural375_zu-23.mdl"}
	
	{bone "gun_rot"
	      {limits 65 -2}
		  {speed2 24}	 
	}
	
	{bone "turret"
	      {limits 150 -155}
		  {speed2 24}	 
	}
}