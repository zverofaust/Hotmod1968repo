{game_entity		;M35A2C
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "multi_board" "truck" "vision_lev02" "tractor" "tractor_heavy"}
	{able "emit_on_attack"}
	{collider "truck"}
	{patherId "gazaaa"}


	;volume, component/tag, thickness

	{Volume "body"
		{thickness 5}
	}
	{Volume "body1"
		{thickness 5}
	}
	{Volume "body2"
		{thickness 5}
	}
	{Volume "body3"
		{thickness 5}
	}
	{Volume "engine"
		{thickness 15}
	}
	
	;volume/component, thickness
	("glass" args "glass_doorl1" 1)
	("glass" args "glass_doorr1" 1)
	("glass" args "glass_front" 1)
	("glass" args "glass_back" 1)

	("fuel" args "fuel")
	("transmission" args "transmission")
	
	("crew_2_human")
	("passenger_10_human")

	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)

	{extender "inventory"
		{Box
		}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{mass 5654}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(58) ;----------------------------------Max speed.
			speedatmaxturn(13) ;-------------------------Max speed at max turn.
			reverse(8) ;--------------------------------Max speed in Reverse.
			weight(5.6) ;------------------------------Weight in Tonnes.
			power(127) ;----------------------------------Horse Power.
			fuel(280) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(480) ;---------------------------------Range in Kilometers.
		)
	}

	{Extension "m35.mdl"}
}